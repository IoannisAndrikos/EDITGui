                    <Button x:Name="loadDicom" Content="Load Ultrasound" HorizontalAlignment="Center" Height="26" Style="{DynamicResource ButtonStyle1}" VerticalAlignment="Center" Width="100" Click="LoadDicom_Click" Margin="0,5,0,0"/>
