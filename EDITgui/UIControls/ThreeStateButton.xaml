             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:EDITgui"
             mc:Ignorable="d" 
             d:DesignHeight="26.167" d:DesignWidth="127.499">
    <Grid Margin="0" MouseLeftButtonDown="Grid_MouseLeftButtonDown">

        <Rectangle x:Name="back" HorizontalAlignment="Center" Height="26.167" RadiusY="6.168" RadiusX="6.168" VerticalAlignment="Center" Width="127.499" Fill="#FF335B42"/>
        <Label x:Name="content" Content="Correction" HorizontalAlignment="Center" Height="17.167" Margin="0" Style="{DynamicResource SimpleLabel}" VerticalAlignment="Center" Width="57.667" Foreground="White"/>

    </Grid>
